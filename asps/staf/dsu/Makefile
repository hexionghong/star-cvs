# Makefile
# $Id: Makefile,v 1.2 1996/09/17 20:54:43 toan Exp $
# Do not edit this file.
#
# CUSTOMIZE Makefile.control -------------------------------------------
-include $(BASE_DIR)/Makefile.control
# 
default: all
SLM_DIR := $(STAR_REF)/sys/mgr/slm
include $(SLM_DIR)/slm.mk
#
# CUSTOMIZE Makefile.user ----------------------------------------------
-include $(BASE_DIR)/Makefile.user
#
.PHONY: default all bin lib test install clean help
#
all: slm_env lib install
#
bin:
	cd bin; $(SLMAKE) bin
#
lib:
	cd lib; $(SLMAKE) lib
#
test: slm_test
	cd lib; $(SLMAKE) test
	cd bin; $(SLMAKE) test
#
install:
	cd lib; $(SLMAKE) install
#
clean:
	cd lib; $(SLMAKE) clean
	cd bin; $(SLMAKE) clean
#
