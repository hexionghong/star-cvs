#!/usr/local/bin/tcsh -f
#
# STAR_LEVELS.csh - C-Shell script to display STAR levels as string.
# Usage: STAR_LEVELS
#
set pgmf = `which root4star`; 
set pgm = `ls -al $pgmf | cut -c42-120 |sed -e 's/ /_/g ; s,/afs/rhic/star/packages/,,g ; s,/bin/root4star,,g'`

echo " "`hostname`";"CERN-$CERN_LEVEL\;STAR-$STAR_LEVEL\;root4star-${pgm}\;`date +%d_%h_%y`|sed -e 's/ //g'
#
