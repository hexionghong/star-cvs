#!/bin/csh
# Set up STAR enviroment
set i = `echo $1 | /bin/awk '{printf("%1.1s\n",$1)}'`
if ("$i" == "9" || "$i" == "0") then
  setenv STAR_LEVEL SL$1
else
  setenv STAR_LEVEL $1
endif
if ( -e $GROUP_DIR/group_env.csh ) then
        source $GROUP_DIR/group_env.csh
	if ( $?prompt ) then
	    if ( ! -e $STAR) then
		echo "$STAR_LEVEL is incompletely defined."
		echo "Could not /usr/bin/find $STAR"
	    endif
	endif
	#echo "$STAR"
	#echo "$STAR_LEVEL"
	#echo "$LD_LIBRARY_PATH"
endif

