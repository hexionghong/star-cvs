#! /bin/csh -f
set list = `ls -d *`
echo $list
foreach dir ($list) 
   cd $dir
   set list2 = `ls -d *`
   echo $list2
   foreach l ($list2)
     if ($l != inc && $l != idl && $l != CVS ) then
       echo $dir $l
       cd $l
       make -f /star/tpc/fisyak/dev/mgr/Makefile clean
       cd ../
     endif 
   end
   cd ../
end

