/*   tcl_tpcluster
* Table of clusters found in tpc
*/
   struct tcl_tpcluster { 
       long      flag;       /* quality flag */
       long      jseq;       /* offset into the sequence table */
       long      nhits;      /* estimated # overlapping hits in cluster */
       long      nseq;       /* number of sequences in this cluster */
       long      tpc_row;    /* 100*sector + row */
   };
